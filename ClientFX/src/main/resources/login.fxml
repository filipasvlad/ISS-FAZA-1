        <Label layoutX="97.0" layoutY="148.0" text="User:" />
        <Label layoutX="92.0" layoutY="192.0" text="Parola:" />
        <TextField fx:id="textUser" layoutX="162.0" layoutY="144.0" />
        <TextField fx:id="textPass" layoutX="162.0" layoutY="188.0" />
        <Label layoutX="190.0" layoutY="65.0" text="LOGIN">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <Button fx:id="butonLogin" layoutX="128.0" layoutY="254.0" mnemonicParsing="false" onAction="#logare" text="Logare" />
        <Button fx:id="butonInregistrare" layoutX="237.0" layoutY="253.0" mnemonicParsing="false" onAction="#inregistrare" text="Inregistrare" />
    </children>
</AnchorPane>
